{
  "name": "argo-clinical",
  "version": "0.3.0",
  "description": "Clinical submission system and repo",
  "scripts": {
    "start": "npm run serve",
    "stop": "pkill --signal 9 clinical",
    "build": "npm run build-ts && npm run resources && npm run tslint",
    "serve": "node dist/src/server.js",
    "watch-node": "nodemon dist/src/server.js",
    "test": "npm run build && mocha --exit --timeout 30000 -r ts-node/register test/**/**/*.ts",
    "int-test": "npm run build && mocha --exit --timeout 30000 -r ts-node/register test/integration/**/*.ts",
    "unit-test": "npm run build && mocha --exit --timeout 5000 -r ts-node/register src/**/unit/*.spec.ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && sleep 3 && npm run watch-debug",
    "resources": "copyfiles -u 1 src/resources/*.* dist/",
    "serve-debug": "nodemon --inspect dist/src/server.js",
    "watch-debug": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec node --inspect-brk -r ts-node/register ./src/server.ts",
    "watch-debug-deprecated": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icgc-argo/argo-clinical.git"
  },
  "author": "blabadi",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/icgc-argo/argo-clinical/issues"
  },
  "homepage": "https://github.com/icgc-argo/argo-clinical#readme",
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^0.0.36",
    "@types/connect-mongo": "^0.0.42",
    "@types/deep-freeze": "^0.1.1",
    "@types/dockerode": "^2.5.20",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.0.10",
    "@types/nodemailer": "^6.2.0",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.8.5",
    "@types/superagent": "^4.1.3",
    "@types/sinon": "4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "husky": "^3.0.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "testcontainers": "^1.1.19",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "async": "^3.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.0.1",
    "lodash": "^4.17.14",
    "mongoose": "^5.6.0",
    "deep-freeze": "^0.0.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2
  }
}
