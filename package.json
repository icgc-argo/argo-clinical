    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.0.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.6.0",
    "nodemailer": "^6.2.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
